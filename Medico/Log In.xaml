<Window x:Class="Medico.Log_In"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Medico"
        mc:Ignorable="d"
        Title="تسجيل الدخول" Icon="logo.png" Height="249.336" Width="411.064" ResizeMode="NoResize" Background="#3b5998" KeyDown="Window_KeyDown">
    <Canvas>
        <Label FlowDirection="RightToLeft" Content="الاسم" Canvas.Left="298" Canvas.Top="44"  FontWeight="Bold" FontSize="20" Foreground="#dfe3ee"/>
        <TextBox x:Name="txtEName" Height="32"  Canvas.Left="18" TextWrapping="Wrap"   Canvas.Top="44" Width="253" Background="#8b9dc3"  FontSize="20" FontWeight="Bold" TabIndex="1"/>
        <PasswordBox x:Name="passbox"  Canvas.Left="16" Height="32" Canvas.Top="108"  FontSize="20" FontWeight="Bold" Background="#8b9dc3"  Width="253"/>
        <Label FlowDirection="RightToLeft" Content="كلمة السر" Foreground="#dfe3ee" Canvas.Left="296" Canvas.Top="103"  FontWeight="Bold" FontSize="20"/>
        <Button Content="تسجيل دخول" Background="#8b9dc3" Canvas.Left="140" Canvas.Top="169" Width="113" Height="30" Click="Button_Click"/>

    </Canvas>
</Window>
